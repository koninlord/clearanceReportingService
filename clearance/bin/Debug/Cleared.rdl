  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ClearedData</DataSourceName>
        <CommandText>SELECT        Student.StudentID, Student.FullName, Fees.FeeAmount, Fees.IsOwing, Fees.AmountOwing, Fees.RecStatus
FROM            Fees INNER JOIN
                         Student ON Fees.StudentID = Student.StudentID
WHERE        (Fees.RecStatus = 'A')</CommandText>
      </Query>
