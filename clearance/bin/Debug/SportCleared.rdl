  <DataSets>
    <DataSet Name="SportClearedList">
      <Query>
        <DataSourceName>SportClearedData</DataSourceName>
        <CommandText>SELECT        Student.StudentID, Student.FullName, Sports.SportsType, Sports.IsOwingKit AS OwingKit, Sports.RecStatus
FROM            Sports INNER JOIN
                         Student ON Sports.StudentID = Student.StudentID
WHERE        (Sports.RecStatus = 'D')</CommandText>
      </Query>
      <Fields>
        <Field Name="StudentID">
          <DataField>StudentID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FullName">
