          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SportsType">
          <DataField>SportsType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OwingKit">
          <DataField>OwingKit</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RecStatus">
          <DataField>RecStatus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
